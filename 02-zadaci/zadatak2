import asyncio
import numpy as np
import psutil

async def afunc1():
    for x in range(10):
        a = np.random.normal(loc=0.0, scale=1.0, size=1000000)
        await asyncio.sleep(0.1)
        print(a)

async def afunc2():
    print(psutil.cpu_percent(10))
    
async def main():
    await afunc1()
    dat = await afunc2()
    print("Iskori≈°tenost CPU-a:", dat)

if __name__ == "__main__":
    asyncio.create_task(main())
